import React from 'react';
import { Switch } from 'react-router-dom';

import ErrorBoundaryRoute from 'app/shared/error/error-boundary-route';

<%_ if (typeof generator['extendList'] != "undefined" &&  generator['extendList'] != "false"){ %>
    import <%= entityReactName %> from './extended/<%= entityFileName %>';
<%_ } else { %>
    import <%= entityReactName %> from './<%= entityFileName %>';
<%_ } %>
<%_ if (typeof generator['extendList'] != "undefined" &&  generator['extendList'] != "false"){ %>
    import <%= entityReactName %>Detail from './extended/<%= entityFileName %>-detail';
<%_ } else { %>
    import <%= entityReactName %>Detail from './<%= entityFileName %>-detail';
<%_ } %>
<%_ if (!readOnly) { _%>
  <%_ if (typeof generator['extendList'] != "undefined" &&  generator['extendList'] != "false"){ %>
    import <%= entityReactName %>Update from './extended/<%= entityFileName %>-update';
  <%_ } else { %>
    import <%= entityReactName %>Update from './<%= entityFileName %>-update';
  <%_ } %>
  import <%= entityReactName %>DeleteDialog from './<%= entityFileName %>-delete-dialog';
<%_ } _%>
<%_ if(openModalRoutes.length > 0 ){  %>
    <%_ for(idx in openModalRoutes){  %>
        import <%= _.startCase(openModalRoutes[idx].split("-").join(" ")).split(" ").join("") %>    from '../<%= openModalRoutes[idx] %>/index';
    <%_ } %>
<%_ } %>

const Routes = ({ match }) => (
  <>
    <Switch>
      <%_ if(openModalRoutes.length > 0 ){  %>
        <%_ for(idx in openModalRoutes){  %>
          <ErrorBoundaryRoute path={`${match.path}/:id<%= entityReactName %>/<%= openModalRoutes[idx] %>`} component={<%= _.startCase(openModalRoutes[idx].split("-").join(" ")).split(" ").join("") %>} />
        <%_ } %>
      <%_ } %>
      <%_ if (!readOnly) { _%>
      <ErrorBoundaryRoute exact path={`${match.path}/new`} component={<%= entityReactName %>Update} />
      <ErrorBoundaryRoute exact path={`${match.path}/:id/edit`} component={<%= entityReactName %>Update} />
      <%_ } _%>
      <ErrorBoundaryRoute exact path={`${match.path}/:id`} component={<%= entityReactName %>Detail}  />
      <ErrorBoundaryRoute path={match.path} component={<%= entityReactName %>} />
    </Switch>
    <%_ if (!readOnly) { _%>
      <ErrorBoundaryRoute path={`${match.path}/:id/delete`} component={<%= entityReactName %>DeleteDialog} />
        <%_ } _%>
        
    <%_ if(openModalRoutes.length > 0 ){  %>
      <Switch>
        <%_ for(idx in openModalRoutes){  %>
            { /* <ErrorBoundaryRoute path={`${match.path}/:id<%= entityReactName %>/<%= openModalRoutes[idx] %>`} component={<%= _.startCase(openModalRoutes[idx].split("-").join(" ")).split(" ").join("") %>} /> */ }
        <%_ } %>
      </Switch>
    <%_ } %>
  </>
);

export default Routes;
