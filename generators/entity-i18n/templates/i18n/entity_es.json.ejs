 -%><%
 let helpBlocks = 0; %>
 {
     "<%= angularAppName %>": {
         "<%= entityTranslationKey %>" : {
            "listSelectedButtons": {
                "clearSelect":"Limpiar Selección",
                "selectAll":"Selecionar os {{totalItems}} <%= entityClassPluralHumanizedEs %>",
                "allItensAreSelected":"Os {{totalItems}} <%= entityClassPluralHumanizedEs %> nesta página estão selecionados.",
                "allItensAreSelectedThisPage":"Todos os {{itemsPerPage}} <%= entityClassPluralHumanizedEs %> nesta página estão selecionadas.",
                "allItensAreSelectedAllPages":"Todas os {{totalItems}} <%= entityClassPluralHumanizedEs %> estão selecionados.",
                <% if (typeof listSelectedButtons !== "undefined") { for (idx in listSelectedButtons) { %>
                    "<%= listSelectedButtons[idx].name %>": "<%= listSelectedButtons[idx].attributes && listSelectedButtons[idx].attributes.text ? listSelectedButtons[idx].attributes.text : listSelectedButtons[idx].name  
                %>",<% } } %>
            },
             "listTopButtons": {
                 <% if (typeof listTopButtons !== "undefined") { for (idx in listTopButtons) { %>
                     "<%= listTopButtons[idx].name %>": "<%= listTopButtons[idx].attributes && listTopButtons[idx].attributes.text ? listTopButtons[idx].attributes.text : listTopButtons[idx].name  
                 %>",<% } } %>
             },
             "listButtons": {
                 <% for (idx in listButtons) { %>
                     "<%= listButtons[idx].name %>": "<%= listButtons[idx].attributes && listButtons[idx].attributes.text ? listButtons[idx].attributes.text : listButtons[idx].name  
                 %>",<% } %>
             },
             "viewButtons": {
                 <% for (idx in viewButtons) { %>
                     "<%= viewButtons[idx].name %>": "<%= viewButtons[idx].attributes && viewButtons[idx].attributes.text ? viewButtons[idx].attributes.text : viewButtons[idx].name  
                 %>",<% } %>
             },
             <%_ for (let i in listTableLayout) { %>
                 <%_ if (listTableLayout[i].type == "sub-relationship" ) { %>
                     "<%= listTableLayout[idx].entity.relationshipName %>": "<%= listTableLayout[idx].attributes && listTableLayout[idx].attributes.text ? listTableLayout[idx].attributes.text : listTableLayout[idx].entity.relationshipNameHumanized  
                     %>",
                 <%_ } %>
             <%_ } %>
             "dropdown_btn": "Acciones",
             "home": {
                 "title": "<%= typeof entityClassPluralHumanizedEs !== 'undefined' ? entityClassPluralHumanizedEs : entityClassPluralHumanized %>",
                 "btn_filter": "Filtrar",
                 "btn_filter_clean": "Limpiar Filtros",
                 "btn_filter_open": "Filtros",
                 "createLabel": "Crear nuevo <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %>",
                 "editLabel": "Editar <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %>",
                 "createOrEditLabel": "Crear o editar <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %>"<% if (searchEngine === 'elasticsearch') { %>,
                 "search": "Buscar <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %>"<% } %>,
                 "notFound": "Ningún <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> encontrado"
             },<% if (!microserviceAppName) { %>
             "created": "Un nuevo <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> foi criado com o identificador {{ param }}",
             "updated": "Un <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> foi atualizado com o identificador {{ param }}",
             "deleted": "Un <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> foi excluído com o identificador {{ param }}",<% } %>
             "delete": {
                 "question": "¿Seguro que quiere eliminar?"
             },
             "field_id": "ID",
             "btnNewItem": "<%= typeof btnNewItemPt !== 'undefined' ? btnNewItemPt : ("Criar novo "  + (typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized) ) %>",
             "btnFilter": "<%= typeof btnFilterPt !== 'undefined' ? btnFilterPt : "Filtrar" %>",
             "btnOpenFilter": "<%= typeof btnOpenFilterPt !== 'undefined' ? btnOpenFilterPt : "Filtrar" %>",
             "btnCleanFilter": "<%= typeof btnCleanFilterPt !== 'undefined' ? btnCleanFilterPt : "Limpar Filtro" %>",
             "btnEditItem": "<%= typeof btnEditItemPt !== 'undefined' ? btnEditItemPt : "Editar" %>",
             "btnActions": "<%= typeof btnActionsPt !== 'undefined' ? btnActionsPt : "Ações" %>",
             "btnSave": "<%= typeof btnSavePt !== 'undefined' ? btnSavePt : "Salvar" %>",
             "btnBack": "<%= typeof btnBackPt !== 'undefined' ? btnBackPt : "Ir na listagem" %>",
             "btnCancel": "<%= typeof btnCancelPt !== 'undefined' ? btnCancelPt : "Cancelar" %>",
             "btnDelete": "<%= typeof btnDeletePt !== 'undefined' ? btnDeletePt : "Apagar" %>",
             "btnOpen": "<%= typeof btnOpenPt !== 'undefined' ? btnOpenPt : "Abrir" %>",
             "createdDate" : "Data criação",
             "lastModifiedDate" : "Data modificação",
             "createdBy" : "Criado por",
             "lastModifiedBy" : "Modificado por",
             <% for (idx in fields) { %>
                <%_ if (['byte[]', 'ByteBuffer'].includes(fields[idx].fieldType)) { _%>
                    "upload_<%= fields[idx].fieldName %>": "Subir <%= fields[idx].fieldNameHumanizedEs ? fields[idx].fieldNameHumanizedEs  : fields[idx].fieldNameHumanized %>",
                <%_ } else if (fields[idx].fieldType === 'Boolean') { _%>
                     "<%= fields[idx].fieldName %>_filter_all": "Indiferente",
                     "<%= fields[idx].fieldName %>_filter_with": "Ativado",
                     "<%= fields[idx].fieldName %>_filter_without": "Desactivado",
                 <% } %>
             <% } %>
             "detail": {
                 "title": "<%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanizedEs %>"
             }<% for (idx in fields) { %>
                <% if (idx in fields) { 
                    %>,"<%= fields[idx].fieldName %>_start": "<%= fields[idx].fieldNameHumanizedEs %> (Inicio)"
                        ,"<%= fields[idx].fieldName %>_end": "<%= fields[idx].fieldNameHumanizedEs %> (Fin)"<%
                    } %>
                <% } %><% for (idx in fields) {
             if (typeof fields[idx].clean_javadoc !== 'undefined') ++helpBlocks; %>,
             "<%= fields[idx].fieldName %>": "<%= fields[idx].fieldNameHumanizedEs ? fields[idx].fieldNameHumanizedEs  : fields[idx].fieldNameHumanized %>"<% } %><% for (idx in relationships) { %>,
             "<%= relationships[idx].relationshipName %>_empty": "Sin <%= relationships[idx].relationshipNameHumanized %>",
             "<%= relationships[idx].relationshipName %>-modal-super-select-title": "Seleccione...",
             "<%= relationships[idx].relationshipName %>": "<%= relationships[idx].relationshipNameHumanized %>"<% } if (helpBlocks > 0) { %>,
             "help": {<% for (idx in fields) {
                 if (fields[idx].clean_javadoc) {
                     --helpBlocks; %>
                 "<%= fields[idx].fieldName %>": "<%= fields[idx].clean_javadoc %>"<% if (helpBlocks > 0) { %>,<% }
                 }
             } %>
             }<% } %>
         }
     }<% if (microserviceAppName) { %>,
     "<%= microserviceAppName %>": {
         "<%= entityTranslationKey %>" : {
             "created": "Un nuevo <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> ha sido creado con el identificador {{ param }}",
             "updated": "Un <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> ha sido actualizado con el identificador {{ param }}",
             "deleted": "Un <%= typeof entityClassHumanizedEs !== 'undefined' ? entityClassHumanizedEs : entityClassHumanized %> ha sido eliminado con el identificador {{ param }}"
         }
     }<% } %>
 }
 